package lista7;

/**
 *
 * @author Michael Andre
 */
public class Registrador_Ip {

   
    String IP;
    int contador;
  

   

    public Registrador_Ip(String IP , int contador) {
        this.IP = IP;
        this.contador = contador;
    }

    
    
    @Override
     public String toString() { 
        return "IP: " + IP + " || " + "NÂ° Acess: " + contador; 
    }
   
     public String getIP() {
        return IP;
    }

    public void setIP(String IP) {
        this.IP = IP;
    }

    public int getContador() {
        return contador;
    }

    public void setContador(int contador) {
        this.contador = contador;
    }

   
}
